		"@smui/select": "^5.0.0-beta.7",
		"@smui/textfield": "^5.0.0-beta.7",
		"@sveltejs/kit": "next",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"sass": "^1.42.1",
